{
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.19.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
